[Unit]
Description=File Transfer Service - Staging

[Service]
Type=forking
EnvironmentFile=-/etc/sysconfig/fts-bringonline
ExecStart=/usr/sbin/fts_bringonline -t 25
KillMode=control-group
TimeoutStopSec=23
KillSignal=SIGTERM
SendSIGHUP=no
SendSIGKILL=yes
Restart=on-failure

[Install]
WantedBy=multi-user.target

