            	</sequence>
            </complexType>
            
            <complexType name="BandwidthLimit">
                <sequence>
                    <element maxOccurs="unbounded" name="blElem" type="config:BandwidthLimitPair"/>
                </sequence>
            </complexType>

            <complexType name="ConfigurationException">
                <sequence>
                    <element name="message" nillable="true" type="xsd:string"/>
                </sequence>
            </complexType>

            <element name="InvalidConfigurationExceptionElement" type="config:InvalidConfigurationException"/>

            <complexType name="InvalidConfigurationException">
                <complexContent>
                    <extension base="config:ConfigurationException">
                        <sequence/>
                    </extension>
                </complexContent>
            </complexType>
            
